<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RegLoginApp.Views.HomePage">
    <ContentPage.Content>

        <StackLayout Orientation="Vertical">
            <Image VerticalOptions="CenterAndExpand" Source="drfeed.png"/>
            <!--Creating a button to Log out-->
            <Button VerticalOptions="EndAndExpand" Text="Logout" TextColor="White" BackgroundColor="DeepSkyBlue" Clicked="Logout_Handler"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>